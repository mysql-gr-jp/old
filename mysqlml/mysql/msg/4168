%define _sysconfdir /etc

Summary: nss_mysql
Summary(ja): MySQL ¤Ç¥¢¥«¥¦¥ó¥È´ÉÍý¤ò¹Ô¤Ê¤¦¥Ñ¥Ã¥±¡¼¥¸
Name: nss_mysql
Version: 0.22
Release: 0vl0
Group: Development/Libraries
License: GPL
Source: <a href="http://prdownloads.sourceforge.net/nss-mysql/%">http://prdownloads.sourceforge.net/nss-mysql/%</a>{name}-%{version}.tar.gz
Patch: nss_mysql-0.22.Makefile.patch
BuildRoot: %{_tmppath}/%{name}-%{version}-%{release}-root

%description
nss_mysql

%prep
%setup -q
%patch -p1 -b .Makefile
perl -i.bak -pe 's@= nss-mysql@= nss_mysql@' nss-mysql.conf

%build
%configure --enable-group --enable-debug #  --enable-shadow
make

%install
rm -rf $RPM_BUILD_ROOT
mkdir -p $RPM_BUILD_ROOT%{_sysconfdir}
mkdir -p $RPM_BUILD_ROOT%{_libdir}
%makeinstall

%clean
rm -rf $RPM_BUILD_ROOT

%files
%defattr(-,root,root)
%doc COPYING INSTALL README SHADOW TODO sample.sql
%{_sysconfdir}/nss-mysql.conf
%attr(600,root,root) %{_sysconfdir}/nss-mysql-root.conf
