&lt;?php
print &quot;てすと、てすとてすと&lt;br&gt;\n&quot;;
print mb_detect_encoding(##DBから取得したデータ##, &quot;auto&quot;);
print ##DBから取得したデータ##;
print mb_convert_encoding(##DBから取得したデータ##, &quot;utf-8&quot;, &quot;auto&quot;);
?&gt;


