# SHOW STATUS
--
-- Table structure for table `param_showstatus`
--

DROP TABLE IF EXISTS `param_showstatus`;
CREATE TABLE `param_showstatus` (
  `s` int(11) NOT NULL auto_increment,
  `mem` varchar(10) default NULL,
  `name` varchar(255) default NULL,
  `def` varchar(255) default NULL,
  `over` varchar(255) default NULL,
  `v32133` varchar(255) default NULL,
  `v32232` varchar(255) default NULL,
  `v32358` varchar(255) default NULL,
  `v40023` varchar(255) default NULL,
  `v40110` varchar(255) default NULL,
  `v50003` varchar(255) default NULL,
  PRIMARY KEY  (`s`),
  KEY `name` (`name`(20))
) ENGINE=MyISAM DEFAULT CHARSET=ujis ;

